#include <stdio.h>

int main(void) {
    int t,a,b,c;
scanf("%d\n",&t);
while(t--)
{
   scanf("%d %d %d\n",&a,&b,&c);
   if(a>b & b>c)
   {
       printf("%d\n",b);
   }
   else if( b>c & c>a)
   {
       printf("%d\n",c);
   }
   else if(c>a & a>b)
   {
       printf("%d\n",a);
   }
}
	return 0;
}

